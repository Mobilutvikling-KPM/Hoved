        <action
            android:id="@+id/action_mineEventFragment_to_eventFragment22"
            app:destination="@id/eventFragment2" />
    </fragment>

    <fragment
        android:id="@+id/nyttEventFragment"
        android:name="com.example.myapplication.fragments.nyttevent.NyttEventFragment"
